<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
		
	<typeAliases>
		<typeAlias type="com.kh.spring.member.model.vo.Member" alias="member"></typeAlias>
		<typeAlias type="com.kh.spring.board.model.vo.Board" alias="board"></typeAlias>
		<typeAlias type="com.kh.spring.notice.model.vo.Notice" alias="notice"></typeAlias>
		<typeAlias type="com.kh.spring.board.model.vo.Reply" alias="reply"></typeAlias>
		<typeAlias type="com.kh.spring.noticeApi.model.vo.Notice" alias="noticeApi"></typeAlias>
	</typeAliases>
	<!-- 주의할점 : 별칭등록을 하기 위해서는 지정해둔 경로에 해당 클레스가 존재해야함 -->
  	
  	
  	
  	<mappers>
  		<mapper resource="/mapper/member-mapper.xml"/>
  		<mapper resource="/mapper/board-mapper.xml"/>
  		<mapper resource="/mapper/notice-mapper.xml"/>
  		<mapper resource="/mapper/noticeApi-mapper.xml"/>
  	</mappers>
  	<!-- 주의할점 : mapper 파일을 등록하기 위해서는 반드시 해당 경로에 파일이 존재해야하며 namespace속성값도 존재해야함 -->
  	
  	

  	
  	
</configuration>